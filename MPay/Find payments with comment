SELECT m.id_operation,  id_provider, id_service, account,  state, substate, time_point, time_server, time_process,ot.value,ot."name" 	
from operations.master m 
join operation_attributes ot on ot.id_operation =m.id_operation 
where ot.name ='comment'
 AND time_process >= '2021-10-01'
  and time_process < '2021-11-03'
  and ot.value is not null
